{
    "name": "clubdeuce/wpmvc-redux",
    "description": "An MVC library for WordPress application development",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daryl",
            "email": "daryl@clubdeuce.net"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Clubdeuce\\Wpmvc_Redux\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clubdeuce\\Wpmvc_Redux\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse --memory-limit=1G"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
